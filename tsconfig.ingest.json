{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": ".ingest-dist",
    "module": "CommonJS",
    "target": "ES2020",
    "lib": ["es2021", "dom"],
    "moduleResolution": "node",
    "noEmit": false,
    "declaration": false,
    "emitDeclarationOnly": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": false
  },
  "include": ["scripts/ingest.ts"],
  "exclude": []
}
